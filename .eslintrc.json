{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "jsx-a11y/anchor-has-content": [
      2,
      {
        "components": ["Anchor"]
      }
    ],
    "react/prop-types": 0
  },
  "env": {
    "browser": true
  }
}
